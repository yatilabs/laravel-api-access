{
    "name": "yatilabs/api-access",
    "description": "Laravel package to provide third party api access to apis",
    "type": "library",
    "keywords": ["laravel", "api", "access", "third-party"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daman Mokha",
            "email": "daman.mokha@yatilabs.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Yatilabs\\ApiAccess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yatilabs\\ApiAccess\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yatilabs\\ApiAccess\\ApiAccessServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "analyse": "phpstan analyse",
        "format": "phpcbf",
        "check-style": "phpcs"
    }
}
