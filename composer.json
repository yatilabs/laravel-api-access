{
    "name": "yatilabs/laravel-api-access",
    "description": "Laravel package for comprehensive API key management with domain restrictions, usage tracking, and secure authentication",
    "type": "library",
    "keywords": ["laravel", "api", "api-key", "authentication", "domain-restriction", "usage-tracking"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daman Mokha",
            "email": "daman.mokha@yatilabs.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Yatilabs\\ApiAccess\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yatilabs\\ApiAccess\\ApiAccessServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
